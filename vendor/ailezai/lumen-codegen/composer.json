    "keywords": ["AiLeZai", "Lumen", "Code-generate"],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=7.1.0",
        "laravel/lumen-framework": "5.*",
        "vlucas/phpdotenv": "~2.2"
    },
    "require-dev": {
    },
    "autoload": {
